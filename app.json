{
  "expo": {
    "name": "FeatherPanel",
    "slug": "featherpanel-mobile-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expo",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0F14"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mobile.featherpanel.com",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "mobile.featherpanel.com"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://featherpanel.com/"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://featherpanel.com/"
      },
      "eas": {
        "projectId": "73184916-7a9c-4e65-802d-c9db1ae1f2dd"
      }
    },
    "owner": "codeforgex_studio"
  }
}
